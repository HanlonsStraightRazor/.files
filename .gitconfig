[core]
    editor = vim
[alias]
    flog = log --oneline --decorate --graph
    ldiff = diff HEAD^ HEAD
[merge]
    tool = vimdiff
[diff]
    tool = vimdiff
[credential]
    helper = cache
[rerere]
    enabled = true
[sendemail]
    annotate = true
[mergetool]
    keepBackup = false
[commit]
    gpgSign = true
[tag]
    gpgSign = true
[init]
	defaultBranch = master
